<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UT.UTDatosServicio">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:paddingTop="50dp"
        android:text="NOMBRE"
        android:id="@+id/UTDatoNombre"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:paddingTop="50dp"
        android:text="NOMBRE"
        android:layout_below="@+id/UTDatoNombre"
        android:id="@+id/UTDatoEstado"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:text="NOMBRE"
        android:paddingTop="50dp"
        android:layout_below="@+id/UTDatoEstado"
        android:id="@+id/UTDatoCarga"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:text="NOMBRE"
        android:paddingTop="50dp"
        android:layout_below="@+id/UTDatoCarga"
        android:id="@+id/UTDatoKm"/>

</RelativeLayout>